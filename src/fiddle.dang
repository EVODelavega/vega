

-- Dang fiddle

Currency: ETH | BTC

EodPriced:
	currency: <Currency>
	EOD: -> Number #currency

Contract:
	currency: <Currency>
	underlying: <EodPriced> =currency
	maturity: date
	size: Integer
	settlement: -> Number #currency

EuroOption <Contract>:
	strike: currency

EuroOption settlement:
	p <- underlying EOD | date == maturity 
	= Settlement amount = p * size - strike * size
